<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/profile_fragment">

    <fragment
        android:id="@+id/profile_fragment"
        android:name="social.entourage.android.new_v8.profile.ProfileFragment">
        <action
            android:id="@+id/action_profile_fragment_to_help_about_fragment"
            app:destination="@id/help_about_fragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:popExitAnim="@anim/slide_out_to_right" />
        <action
            android:id="@+id/action_profile_fragment_to_edit_profile_fragment"
            app:destination="@id/edit_profile_fragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:popExitAnim="@anim/slide_out_to_right" />
        <action
            android:id="@+id/action_profile_fragment_to_association_fragment"
            app:destination="@id/association_fragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:popExitAnim="@anim/slide_out_to_right" />
        <action
            android:id="@+id/action_profile_fragment_to_test_fragment"
            app:destination="@id/test_fragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:popExitAnim="@anim/slide_out_to_right" />
    </fragment>

    <fragment
        android:id="@+id/test_fragment"
        android:name="social.entourage.android.new_v8.TestFragment">
        <action
            android:id="@+id/action_test_fragment_to_user_fragment"
            app:destination="@id/user_fragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:popExitAnim="@anim/slide_out_to_right" />

        <action
            android:id="@+id/action_test_fragment_to_create_group_fragment"
            app:destination="@id/create_group_fragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:popExitAnim="@anim/slide_out_to_right" />
        <action
            android:id="@+id/action_test_fragment_to_edit_fragment"
            app:destination="@id/edit_fragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:popExitAnim="@anim/slide_out_to_right" />
    </fragment>


    <fragment
        android:id="@+id/settings_fragment"
        android:name="social.entourage.android.new_v8.profile.settings.SettingsFragment" />

    <fragment
        android:id="@+id/help_about_fragment"
        android:name="social.entourage.android.new_v8.profile.settings.HelpAboutFragment" />

    <fragment
        android:id="@+id/edit_profile_fragment"
        android:name="social.entourage.android.new_v8.profile.editProfile.EditProfileFragment">
        <action
            android:id="@+id/action_edit_profile_fragment_to_edit_profile_interest_fragment"
            app:destination="@id/edit_profile_interest_fragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:popExitAnim="@anim/slide_out_to_right" />
        <action
            android:id="@+id/action_edit_profile_fragment_to_choose_photo_fragment"
            app:destination="@id/edit_profile_image_fragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:popExitAnim="@anim/slide_out_to_right" />
        <action
            android:id="@+id/action_edit_profile_fragment_to_edit_action_zone_fragment"
            app:destination="@id/edit_action_zone_fragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:popExitAnim="@anim/slide_out_to_right" />
    </fragment>

    <fragment
        android:id="@+id/edit_profile_interest_fragment"
        android:name="social.entourage.android.new_v8.profile.editProfile.EditInterestsFragment" />


    <fragment
        android:id="@+id/edit_profile_image_fragment"
        android:name="social.entourage.android.user.edit.photo.ChoosePhotoFragment" />


    <fragment
        android:id="@+id/edit_action_zone_fragment"
        android:name="social.entourage.android.user.edit.place.UserEditActionZoneFragment" />

    <fragment
        android:id="@+id/association_fragment"
        android:name="social.entourage.android.new_v8.association.AssociationProfile">
        <argument
            android:name="partner_id"
            app:argType="integer" />
    </fragment>


    <fragment
        android:id="@+id/create_group_fragment"
        android:name="social.entourage.android.new_v8.group.create.CreateGroupFragment">
        <action
            android:id="@+id/action_create_group_fragment_to_create_group_success_fragment"
            app:destination="@id/create_group_success_fragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:popExitAnim="@anim/slide_out_to_right" />
    </fragment>

    <fragment
        android:id="@+id/user_fragment"
        android:name="social.entourage.android.new_v8.user.UserProfileFragment">
        <argument
            android:name="user_id"
            app:argType="integer" />
        <action
            android:id="@+id/action_user_fragment_to_report_user_fragment"
            app:destination="@id/report_user_fragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:popExitAnim="@anim/slide_out_to_right" />
    </fragment>

    <fragment
        android:id="@+id/report_user_fragment"
        android:name="social.entourage.android.new_v8.user.ReportUserModalFragment" />


    <fragment
        android:id="@+id/create_group_success_fragment"
        android:name="social.entourage.android.new_v8.group.create.CreateGroupSuccessFragment" />


    <fragment
        android:id="@+id/edit_fragment"
        android:name="social.entourage.android.new_v8.group.edit.EditGroupFragment" />

</navigation>