<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="@color/white"
android:clickable="true"
android:focusable="true"
android:focusableInTouchMode="true"
android:id="@+id/onboard_asso_fill_mainlayout"
tools:context="social.entourage.android.onboarding.asso.OnboardingAssoFillFragment">


    <TextView
        android:id="@+id/ui_onboard_email_tv_title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:text="@string/onboard_asso_fill_title"
        android:textColor="@color/onboard_black_36"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ui_onboard_email_tv_description"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="sans-serif-light"
        android:text="@string/onboard_asso_fill_description"
        android:textColor="@color/onboard_black_36"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/ui_onboard_email_tv_title"
        app:layout_constraintTop_toBottomOf="@+id/ui_onboard_email_tv_title" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/ui_layout_asso_fill_location"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:background="@color/onboard_textfield_bg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ui_onboard_email_tv_description">

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_onboard_picto_search" />

        <TextView
            android:id="@+id/ui_onboard_asso_fill_asso_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginEnd="8dp"
            android:text="@string/onboard_asso_fill_name_placeholder"
            android:textColor="@color/quantum_grey"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageView10"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <EditText
        android:id="@+id/ui_onboard_asso_fill_postal_code"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:background="@color/onboard_textfield_bg"
        android:ems="10"
        android:textColorHint="@color/quantum_grey"
        android:hint="@string/onboard_asso_fill_cp_placeholder"
        android:inputType="textEmailAddress"
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        android:textColor="@color/pre_onboard_black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ui_layout_asso_fill_location" />

    <EditText
        android:id="@+id/ui_onboard_asso_fill_function"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="16dp"
        android:background="@color/onboard_textfield_bg"
        android:ems="10"
        android:textColorHint="@color/quantum_grey"
        android:hint="@string/onboard_asso_fill_function_placeholder"
        android:inputType="textEmailAddress"
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        android:textColor="@color/pre_onboard_black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ui_onboard_asso_fill_postal_code" />

    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/pre_onboard_black"
        app:layout_constraintBottom_toBottomOf="@+id/ui_onboard_asso_fill_function"
        app:layout_constraintEnd_toEndOf="@+id/ui_onboard_asso_fill_function"
        app:layout_constraintStart_toStartOf="@+id/ui_onboard_asso_fill_function">

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout3"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/pre_onboard_black"
        app:layout_constraintBottom_toBottomOf="@+id/ui_onboard_asso_fill_postal_code"
        app:layout_constraintEnd_toEndOf="@+id/ui_onboard_asso_fill_postal_code"
        app:layout_constraintStart_toStartOf="@+id/ui_onboard_asso_fill_postal_code">

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout4"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/pre_onboard_black"
        app:layout_constraintBottom_toBottomOf="@+id/ui_layout_asso_fill_location"
        app:layout_constraintEnd_toEndOf="@+id/ui_layout_asso_fill_location"
        app:layout_constraintStart_toStartOf="@+id/ui_layout_asso_fill_location">

    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
