        <item quantity="one">%1$d personne souhaite rejoindre votre maraude.</item>
        <item quantity="other">%1$d personnes souhaitent rejoindre votre maraude.</item>
    </plurals>
    <plurals name="notification_text_join_request_entourage_multiple">
