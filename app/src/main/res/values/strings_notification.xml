    <plurals name="notification_text_chat_message">
        <item quantity="zero">Vous n'avez pas de nouveau message</item>
        <item quantity="one">Vous avez %1$d nouveau message</item>
        <item quantity="other">Vous avez %1$d nouveaux messages</item>
    </plurals>

    <string name="notification_text_join_request_tour_single">@string/tour_join_request_received_message</string>
    <string name="notification_text_join_request_entourage_single">@string/entourage_join_request_received</string>
