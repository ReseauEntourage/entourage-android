<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HtmlTextView">
        <attr name="htmlText" format="string" />
    </declare-styleable>
    <declare-styleable name="MainProfileItemView">
        <attr name="showRightArrow" format="boolean"/>
        <attr name="centerText" format="boolean" />
        <attr name="android:icon" />
        <attr name="iconTint" />
        <attr name="android:title" />
        <attr name="android:textColor" />
        <attr name="android:textSize" />
        <attr name="textBackground" format="reference"/>
    </declare-styleable>
    <declare-styleable name="CountryCodePicker">
        <attr format="integer" name="ccp_defaultCode"/>
        <attr format="color" name="ccp_textColor"/>
        <attr format="color" name="ccp_backgroundColor"/>
        <attr format="dimension" name="ccp_textSize"/>
        <attr format="string" name="ccp_textFont"/>
        <attr format="dimension" name="ccp_arrowSize"/>
        <attr format="string" name="ccp_countryPreference"/>
        <attr format="string" name="ccp_defaultNameCode"/>
        <attr format="boolean" name="ccp_hideNameCode"/>
        <attr format="boolean" name="ccp_keyboardAutoPopOnSearch"/>
        <attr format="string" name="ccp_customMasterCountries"/>
        <attr format="boolean" name="ccp_showFlag"/>
        <attr format="boolean" name="ccp_showFullName"/>
        <attr format="boolean" name="ccp_selectionDialogShowSearch"/>
        <attr format="boolean" name="ccp_clickable"/>
        <attr format="boolean" name="ccp_hidePhoneCode"/>
        <attr format="boolean" name="ccp_enablePhoneAutoFormatter"/>
        <attr format="boolean" name="ccp_setCountryByTimeZone"/>
        <attr format="boolean" name="ccp_alternative_layout"/>
    </declare-styleable>
    <declare-styleable name="CustomTitleView">
        <attr name="entourageTitle" format="string"/>
        <attr name="entourageTitleCloseDrawable" format="reference"/>
        <attr name="entourageTitleCloseDrawableTint" format="reference"/>
        <attr name="entourageTitleAction" format="string"/>
        <attr name="entourageShowSeparator" format="boolean"/>
        <attr name="android:background"/>
        <attr name="android:textColor"/>
    </declare-styleable>
    <declare-styleable name="HalfCircleView">
        <attr name="shapeColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="UserNameView">
        <attr name="android:textColor"/>
        <attr name="android:textSize"/>
    </declare-styleable>

    <declare-styleable name="ProgressBarView">
        <attr name="percent" format="float"/>
    </declare-styleable>
</resources>